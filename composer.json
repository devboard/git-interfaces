{
    "name": "devboard/git-interfaces",
    "description": "Devboard git interfaces",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Git\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "infection/infection": "^0.15.1"
    }
}
